{
  "name": "@hubot-friends/hubot-ms-teams",
  "version": "0.0.0-development",
  "description": "A new MS Teams adapter for Hubot",
  "main": "index.mjs",
  "bin": {
    "hubot-ms-teams": "bin/hubot-ms-teams.mjs"
  },
  "scripts": {
    "test": "node --test",
    "start": "node --watch --env-file=.env node_modules/.bin/hubot -f ./index.mjs -n test-bot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubot-friends/hubot-ms-teams.git"
  },
  "keywords": [
    "hubot",
    "ms teams",
    "chat",
    "bot"
  ],
  "author": "Joey Guerra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hubot-friends/hubot-ms-teams/issues"
  },
  "homepage": "https://github.com/hubot-friends/hubot-ms-teams#readme",
  "peerDependencies": {
    "hubot": ">= 12"
  },
  "engines": {
    "node": ">= 20"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "dryRun": false
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@microsoft/teams.ai": "^2.0.1",
    "@microsoft/teams.api": "^2.0.1",
    "@microsoft/teams.botbuilder": "^2.0.1",
    "botbuilder": "4.23.1",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "hubot-diagnostics": "^2.1.3",
    "hubot-help": "^2.1.5",
    "hubot-rules": "^2.1.1",
    "jsonc-parser": "^3.2.1"
  }
}
